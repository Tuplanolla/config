# The classic sRGB primaries (especially yellow) can be difficult to see,
# which is why we replace them with the ITU-R BT.601 primaries.
set linetype 1 linecolor rgb '#b30000'
set linetype 6 linecolor rgb '#8e8e00'
set linetype 2 linecolor rgb '#006900'
set linetype 5 linecolor rgb '#00a6a6'
set linetype 3 linecolor rgb '#0000e2'
set linetype 4 linecolor rgb '#ca00ca'
set linetype cycle 6
